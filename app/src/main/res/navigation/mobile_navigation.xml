<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.mapped.ui.findplaces.FindPlacesFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_myplaces"
        android:name="com.example.mapped.ui.places.PlacesFragment"
        android:label="My Places"
        tools:layout="@layout/fragment_places" />

    <fragment
        android:id="@+id/navigation_messaging"
        android:name="com.example.mapped.ui.messaging.MessagingFragment"
        android:label="Messaging"
        tools:layout="@layout/fragment_messages" />

    <fragment
        android:id="@+id/navigation_profil"
        android:name="com.example.mapped.ui.profil.ProfilFragment"
        android:label="Profil"
        tools:layout="@layout/fragment_profil" />
</navigation>